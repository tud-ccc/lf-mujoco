target Cpp;

public preamble {=

#ifndef RANDOM_START_POINT_GENERATOR
#define RANDOM_START_POINT_GENERATOR
#include "common_vector.hpp"


#endif //RANDOM_START_POINT_GENERATOR
=}


reactor RandomStartPointGenerator {


  //robo facing
  input calculation_finished: void;
  output random_next_position_triplet: PositionTriplet;


  reaction(calculation_finished) ->random_next_position_triplet{=
      std::cout << "Generated new Start point" << std::endl; 
      PositionTriplet pt = new PositionTriplet{Vector{1,1,0},Vector{0,0,0},Vector{-1,-1,0}};
      random_next_position_triplet.set(pt);
  =}
}