target Cpp {
    cmake-include: "testing.cmake"
  }

  import Planner from "../../src/control/Planner.lf"
  import PlannerTester from "tester/PlannerTester.lf"

  public preamble {=
    #ifndef TEST_START_UP
    #define TEST_START_UP
  
    #endif //XARM7
  =}
  
  
  main reactor TestStartUp {
    planner = new Planner()
    planner_tester = new PlannerTester()

    planner.next_position_checked -> planner_tester.planner_tester_next_position

    planner_tester.planner_raw_instruction ->planner.raw_instruction
    planner_tester.planner_current_position_planner ->planner.current_position_planner
    planner_tester.planner_last_position_planner ->planner.last_position_planner



  }
  